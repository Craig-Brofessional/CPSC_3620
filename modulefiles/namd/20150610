#%Module1.0
##
## namd/20150610  modulefile
##
proc ModulesHelp { } {
    puts stderr "namd/20150610 - sets the environment for NAMD CVS version from 2015-06-10 (pre 2.11)"
}

module-whatis   "sets the environment for NAMD 20150610 version (pre 2.11)"

module add gcc/4.8.1 openmpi/1.8.4 fftw/3.3.4-g481

prepend-path PATH /software/namd/20150610

