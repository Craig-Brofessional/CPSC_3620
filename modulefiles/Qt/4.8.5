#%Module1.0#####################################################################
##
## Qt/4.8.5  modulefile
##
proc ModulesHelp { } {
        global version

        puts stderr "\n\tQt module"
        puts stderr "\t****************************************************"
        puts stderr "\n\t  This module sets up the following environment"
        puts stderr "\t  variables for the samtools binaries:"
        puts stderr "\t      PATH"
        puts stderr "\n\t  Version $version\n"
        puts stderr "\t****************************************************\n"
}

module-whatis	"Set up environment for Qt"

